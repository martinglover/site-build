{
    "name": "Octo Setup",
    "description": "Octo site setup",
    "authors": [
        {
            "name": "Martin Glover",
            "email": "dev@mjglover.co.uk"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "octo/skeleton",
                "type": "webroot",
                "version": "master",
                "source": {
                    "type": "git",
                    "url": "git@bitbucket.org:block8/octo-skeleton.git",
                    "reference": "master"
                },
                "require" : {
                    "fancyguy/webroot-installer": "1.0.0"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "octo/skeleton": "*"
    },
    "extra": {
        "webroot-dir": "site",
        "webroot-package": "octo/skeleton"
    },
    "minimum-stability": "dev"
}