deploy.env=sandbox

deploy.site.name=${sitename}
deploy.site.url=${sitename}.dev.co.uk
deploy.site.alias=www.${sitename}.dev.co.uk
deploy.site.path=/var/www/${sitename}
deploy.site.email=email@dev.co.uk

deploy.author.name=Author
deploy.author.url=http://dev.co.uk
deploy.author.email=email@dev.co.uk

deploy.cms.username=username
deploy.cms.password=password

deploy.db.name=${sitename}
deploy.db.username=username
deploy.db.password=password
deploy.db.host=localhost

platform.silverstripe.version=3.1.5

platform.wordpress.version=3.9
platform.wordpress.language=en_UK

platform.ghost.env=production

platform.zf2.version=2.3.1

webserver.apache.path=/etc/apache2

webserver.nginx.path=/etc/nginx